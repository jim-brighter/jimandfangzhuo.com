{
  "name": "jimandfangzhuo.com",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "planner": "cdk/planner.js"
  },
  "scripts": {
    "cdk:build": "tsc",
    "cdk:watch": "tsc -w",
    "cdk:test": "jest",
    "cdk": "cdk",
    "cdk:deploy:hostedzone": "cdk deploy PlannerHostedZone --require-approval never --outputs-file hosted-zone-outputs.json",
    "cdk:destroy:hostedzone": "cdk destroy PlannerHostedZone -f",
    "cdk:deploy:backend": "cdk deploy PlannerBackend --require-approval never --outputs-file backend-outputs.json",
    "cdk:destroy:backend": "cdk destroy PlannerBackend -f",
    "cdk:deploy:frontend": "cdk deploy PlannerFrontend --require-approval never --outputs-file frontend-outputs.json",
    "cdk:destroy:frontend": "cdk destroy PlannerFrontend -f",
    "cdk:deploy": "cdk deploy PlannerBackend PlannerFrontend --require-approval never --outputs-file frontend-backend-outputs.json",
    "ng": "ng",
    "angular:start": "ng serve --watch",
    "angular:build": "ng build",
    "angular:build:prod": "ng build -c production --output-path=dist",
    "angular:test": "ng test",
    "angular:lint": "ng lint",
    "angular:e2e": "ng e2e",
    "deploy": "npm run angular:build:prod && npm run cdk:deploy"
  },
  "dependencies": {
    "@angular/animations": "^15.1.1",
    "@angular/cli": "^15.2.4",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.1.1",
    "@angular/core": "^15.1.1",
    "@angular/forms": "^15.2.4",
    "@angular/platform-browser": "^15.1.1",
    "@angular/platform-browser-dynamic": "^15.2.4",
    "@angular/router": "^15.2.5",
    "@aws-amplify/ui-angular": "^3.2.4",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "aws-amplify": "^5.0.11",
    "aws-cdk-lib": "2.61.1",
    "aws-sdk": "^2.1299.0",
    "constructs": "^10.1.289",
    "core-js": "^3.27.2",
    "ng2-file-upload": "^3.0.0",
    "rxjs": "^7.8.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.4",
    "@angular/compiler-cli": "^15.1.1",
    "@angular/language-service": "^15.1.1",
    "@types/aws-lambda": "^8.10.114",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.15.11",
    "@types/prettier": "2.7.2",
    "aws-cdk": "2.61.1",
    "codelyzer": "^6.0.2",
    "csv-parser": "^3.0.0",
    "esbuild": "^0.17.3",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^29.3.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "~7.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "^4.9.4"
  }
}
