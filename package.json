{
  "name": "jimandfangzhuo.com",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "planner": "cdk/planner.js"
  },
  "scripts": {
    "cdk:build": "tsc",
    "cdk:watch": "tsc -w",
    "cdk:test": "jest",
    "cdk": "cdk",
    "cdk:deploy:hostedzone": "cdk deploy PlannerHostedZone --require-approval never --outputs-file hosted-zone-outputs.json",
    "cdk:destroy:hostedzone": "cdk destroy PlannerHostedZone -f",
    "cdk:deploy:backend": "cdk deploy PlannerBackend --require-approval never --outputs-file backend-outputs.json",
    "cdk:destroy:backend": "cdk destroy PlannerBackend -f",
    "cdk:deploy:frontend": "cdk deploy PlannerFrontend --require-approval never --outputs-file frontend-outputs.json",
    "cdk:destroy:frontend": "cdk destroy PlannerFrontend -f",
    "cdk:deploy": "npm run cdk:deploy:backend && npm run cdk:deploy:frontend",
    "ng": "ng",
    "angular:start": "ng serve --watch",
    "angular:build": "ng build",
    "angular:build:prod": "NODE_OPTIONS='--max-old-space-size=1024' ng build -c production --output-path=dist",
    "angular:test": "ng test",
    "angular:lint": "ng lint",
    "angular:e2e": "ng e2e",
    "deploy": "npm run angular:build:prod && npm run cdk:deploy"
  },
  "dependencies": {
    "@angular/animations": "^14.0.6",
    "@angular/cli": "^14.0.6",
    "@angular/common": "^14.0.6",
    "@angular/compiler": "^14.0.6",
    "@angular/core": "^14.0.6",
    "@angular/forms": "^14.0.6",
    "@angular/platform-browser": "^14.0.6",
    "@angular/platform-browser-dynamic": "^14.0.6",
    "@angular/router": "^14.0.6",
    "@aws-amplify/ui-angular": "^2.4.14",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "aws-amplify": "^4.3.27",
    "aws-cdk-lib": "2.32.1",
    "aws-sdk": "^2.1176.0",
    "constructs": "^10.1.47",
    "core-js": "^3.23.4",
    "ng2-file-upload": "^1.4.0",
    "rxjs": "^7.5.6",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.6",
    "@angular/compiler-cli": "^14.0.6",
    "@angular/language-service": "^14.0.6",
    "@types/aws-lambda": "^8.10.101",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.5",
    "@types/prettier": "2.6.3",
    "aws-cdk": "2.32.1",
    "codelyzer": "^6.0.2",
    "esbuild": "^0.14.49",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^28.1.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "~7.0.0",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "^4.7.4"
  }
}
