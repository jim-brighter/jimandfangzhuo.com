{
  "name": "jimandfangzhuo.com",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "planner": "cdk/planner.js"
  },
  "scripts": {
    "react:build": "react-scripts build",
    "react:start": "react-scripts start",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "cdk:build": "tsc",
    "cdk:watch": "tsc -w",
    "cdk:test": "jest",
    "cdk": "cdk",
    "cdk:deploy:hostedzone": "cdk deploy PlannerHostedZone --require-approval never --outputs-file hosted-zone-outputs.json",
    "cdk:destroy:hostedzone": "cdk destroy PlannerHostedZone -f",
    "cdk:deploy:backend": "cdk deploy PlannerBackend --require-approval never --outputs-file backend-outputs.json",
    "cdk:destroy:backend": "cdk destroy PlannerBackend -f",
    "cdk:deploy:frontend": "cdk deploy PlannerFrontend --require-approval never --outputs-file frontend-outputs.json",
    "cdk:destroy:frontend": "cdk destroy PlannerFrontend -f"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "aws-cdk-lib": "2.30.0",
    "aws-sdk": "^2.1168.0",
    "constructs": "^10.1.43",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.2",
    "@types/prettier": "2.6.3",
    "aws-cdk": "2.30.0",
    "esbuild": "^0.14.48",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
