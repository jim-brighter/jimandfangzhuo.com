{
  "name": "jimandfangzhuo.com",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "planner": "cdk/planner.js"
  },
  "scripts": {
    "cdk:build": "tsc",
    "cdk:watch": "tsc -w",
    "cdk:test": "jest",
    "cdk": "cdk",
    "cdk:deploy:hostedzone": "cdk deploy PlannerHostedZone --require-approval never --outputs-file hosted-zone-outputs.json",
    "cdk:destroy:hostedzone": "cdk destroy PlannerHostedZone -f",
    "cdk:deploy:backend": "cdk deploy PlannerBackend --require-approval never --outputs-file backend-outputs.json",
    "cdk:destroy:backend": "cdk destroy PlannerBackend -f",
    "cdk:deploy:frontend": "cdk deploy PlannerFrontend --require-approval never --outputs-file frontend-outputs.json",
    "cdk:destroy:frontend": "cdk destroy PlannerFrontend -f",
    "cdk:deploy": "cdk deploy PlannerBackend PlannerFrontend --require-approval never --outputs-file frontend-backend-outputs.json",
    "ng": "ng",
    "angular:start": "ng serve --watch",
    "angular:build": "ng build",
    "angular:build:prod": "ng build -c production --output-path=dist",
    "angular:test": "ng test",
    "angular:lint": "ng lint",
    "angular:e2e": "ng e2e",
    "deploy": "npm run angular:build:prod && npm run cdk:deploy"
  },
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cli": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@aws-amplify/ui-angular": "^3.2.15",
    "@aws-sdk/client-dynamodb": "^3.329.0",
    "@aws-sdk/lib-dynamodb": "^3.329.0",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "aws-amplify": "^5.2.0",
    "aws-cdk-lib": "2.78.0",
    "constructs": "^10.2.17",
    "core-js": "^3.30.2",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@angular/language-service": "^16.0.0",
    "@types/aws-lambda": "^8.10.115",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.1",
    "@types/prettier": "2.7.2",
    "aws-cdk": "2.78.0",
    "codelyzer": "^6.0.2",
    "csv-parser": "^3.0.0",
    "esbuild": "^0.17.18",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^29.5.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "~7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "^5.0.4"
  }
}
