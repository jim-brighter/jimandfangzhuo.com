{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:hostedzone": "cdk deploy PlannerHostedZone --require-approval never --outputs-file hosted-zone-outputs.json",
    "destroy:hostedzone": "cdk destroy PlannerHostedZone -f",
    "deploy:backend": "cdk deploy PlannerBackend --require-approval never --outputs-file backend-outputs.json",
    "destroy:backend": "cdk destroy PlannerBackend -f"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@types/prettier": "2.6.3",
    "aws-cdk": "2.30.0",
    "esbuild": "^0.14.48",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.30.0",
    "aws-sdk": "^2.1167.0",
    "constructs": "^10.1.43",
    "source-map-support": "^0.5.21"
  }
}
